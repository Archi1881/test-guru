<h2>Test <%= @test.title %> have <%= 'question'.pluralize(@test.questions.count) %></h2>
<p>
  <strong>Title:</strong>
  <%= @test.title %>
</p>

<p>
  <strong>Level:</strong>
  <%= @test.level %>
</p>

<p>
  <% @test_questions.each do |question| %>
    <li>
      <%= question.body %>
    </li>
  <% end %>
</p>

<ol>
  <%= render partial: 'questions/question', collection: @test.questions %>
</ol>

<%= link_to 'Edit', edit_admin_test_path(@test) %>
<%= link_to 'Back', admin_tests_path %>