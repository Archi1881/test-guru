<h2> Test <%= @test.title %> have <%= 'question'.pluralize(@test.questions.count) %></h2>
<p>
  <strong><%= t('.title') %></strong>
  <%= @test.title %>
</p>

<p>
  <strong>Level:</strong>
  <strong><%= t('.level') %></strong>
</p>

<p>
  <h2> <%= t('.questions') %></h2>

  <%= link_to t('.new_question'), new_admin_test_question_path(@test) %>
  
  <table>
    <%= render partial: 'admin/questions/question', collection: @test.questions %>
  </table>
</p>

<%= link_to t('helpers.edit'), edit_admin_test_path(@test) %>
<%= link_to t('helpers.back'), admin_tests_path %>